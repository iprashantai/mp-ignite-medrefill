    "interval_merging_tests": {
      "comment": "Test cases for F063: Coverage period merging",
      "test_non_overlapping": {
        "description": "F063-TC01: Non-overlapping fills counted separately",
        "resources": [
          {
            "resourceType": "MedicationDispense",
            "id": "merge-test-001",
            "status": "completed",
            "subject": { "reference": "Patient/MERGE_TEST" },
            "whenHandedOver": "2025-01-01",
            "daysSupply": { "value": 30 }
          },
          {
            "resourceType": "MedicationDispense",
            "id": "merge-test-002",
            "status": "completed",
            "subject": { "reference": "Patient/MERGE_TEST" },
            "whenHandedOver": "2025-02-01",
            "daysSupply": { "value": 28 }
          }
        ],
        "expected": {
          "coveredDays": 58,
          "comment": "No overlap: Jan 1-30 (30 days) + Feb 1-28 (28 days) = 58 days"
        }
      },
      "test_overlapping": {
        "description": "F063-TC02: Overlapping fills merged correctly",
        "resources": [
          {
            "resourceType": "MedicationDispense",
            "id": "merge-test-003",
            "status": "completed",
            "subject": { "reference": "Patient/MERGE_TEST" },
            "whenHandedOver": "2025-01-01",
            "daysSupply": { "value": 30 }
          },
          {
            "resourceType": "MedicationDispense",
            "id": "merge-test-004",
            "status": "completed",
            "subject": { "reference": "Patient/MERGE_TEST" },
            "whenHandedOver": "2025-01-15",
            "daysSupply": { "value": 30 }
          }
        ],
        "expected": {
          "coveredDays": 44,
          "comment": "Overlap: Jan 1-30 merged with Jan 15-Feb 13 = Jan 1-Feb 13 (44 days)"
        }
      },
      "test_fully_overlapping": {
        "description": "F063-TC03: Fully overlapping fill handled",
        "resources": [
          {
            "resourceType": "MedicationDispense",
            "id": "merge-test-005",
            "status": "completed",
            "subject": { "reference": "Patient/MERGE_TEST" },
            "whenHandedOver": "2025-01-01",
            "daysSupply": { "value": 60 }
          },
          {
            "resourceType": "MedicationDispense",
            "id": "merge-test-006",
            "status": "completed",
            "subject": { "reference": "Patient/MERGE_TEST" },
            "whenHandedOver": "2025-01-15",
            "daysSupply": { "value": 15 }
          }
        ],
        "expected": {
          "coveredDays": 60,
          "comment": "Second fill completely inside first: Jan 1-Mar 1 (60 days)"
        }
      },
      "test_year_end_cap": {
        "description": "F063-TC04: Cap at Dec 31",
        "resources": [
          {
            "resourceType": "MedicationDispense",
            "id": "merge-test-007",
            "status": "completed",
            "subject": { "reference": "Patient/MERGE_TEST" },
            "whenHandedOver": "2025-12-01",
            "daysSupply": { "value": 90 }
          }
        ],
        "expected": {
          "coveredDays": 31,
          "comment": "Supply extends past Dec 31, capped at year end: Dec 1-31 (31 days)"
        }
      }
    }
